{
  "name": "action-delete-ghcr-untagged",
  "version": "1.0.0",
  "private": true,
  "description": "GitHub action to delete untagged version of a package in GitHub Container Registry",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build src/main.ts",
    "test": "echo \"Haven't think of how to test\"",
    "all": "npm run build && npm run format && npm run lint && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/duskmoon314/action-delete-ghcr-untagged.git"
  },
  "keywords": [
    "actions",
    "node"
  ],
  "author": "duskmoon314 (Campbell He) <kp.campbell.he@duskmoon314.com>",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@octokit/core": "^4.0.5",
    "@octokit/rest": "^19.0.4",
    "dayjs": "^1.11.5"
  },
  "devDependencies": {
    "@octokit/types": "^8.0.0",
    "@types/node": "^18.8.3",
    "@typescript-eslint/parser": "^5.38.1",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.24.0",
    "eslint-plugin-github": "^4.3.7",
    "eslint-plugin-jest": "^27.0.4",
    "jest": "^27.2.5",
    "js-yaml": "^4.1.0",
    "prettier": "2.7.1",
    "ts-jest": "^27.1.2",
    "typescript": "^4.8.4"
  }
}